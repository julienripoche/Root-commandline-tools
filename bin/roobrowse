#!/usr/bin/python

"""Commandline to ..."""

from cmdLineUtils import *
from cmdLineHelps import *

##### Beginning of the main code #####

# Collect arguments with the module argparse
parser = argparse.ArgumentParser(description=ROOBROWSE_HELP)
parser.add_argument("sourcePatternList", help=ARG_HELP)
args = parser.parse_args()

with stdoutRedirected(to=os.devnull), mergedStderrStdout():
    rootFile = ROOT.TFile.Open(args.sourcePattern)
rootFile.Browse(ROOT.TBrowser())
input()
rootFile.Close()
